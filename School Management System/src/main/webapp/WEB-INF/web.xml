<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
      
       <servlet>
          <servlet-name>login</servlet-name>
          <servlet-class>net.java.schoolmanagement.web.AdminloginServlet</servlet-class>
       </servlet>
       <servlet-mapping>
          <servlet-name>login</servlet-name>
          <url-pattern>/log</url-pattern>
       </servlet-mapping>

      
      
      
       <servlet>
          <servlet-name>StudentServlet</servlet-name>
          <servlet-class>net.java.schoolmanagement.web.StudentServlet</servlet-class>
       </servlet>
       <servlet-mapping>
          <servlet-name>StudentServlet</servlet-name>
          <url-pattern>/student/*</url-pattern>
       </servlet-mapping>
       
      
     <servlet>
          <servlet-name>TeacherServlet</servlet-name>
          <servlet-class>net.java.schoolmanagement.web.TeacherServlet</servlet-class>
       </servlet>
       <servlet-mapping>
          <servlet-name>TeacherServlet</servlet-name>
          <url-pattern>/teacher/*</url-pattern>
       </servlet-mapping>
       
      <servlet>
          <servlet-name>PrincipalServlet</servlet-name>
          <servlet-class>net.java.schoolmanagement.web.PrincipalServlet</servlet-class>
       </servlet>
       <servlet-mapping>
          <servlet-name>PrincipalServlet</servlet-name>
          <url-pattern>/principal/*</url-pattern>
       </servlet-mapping>
       
       
          
</web-app>